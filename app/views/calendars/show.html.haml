%div.container
  %div.row
    %div.col-md-9
      %ul
        - @appointments.each do |appointment|
          %li= appointment.name
        - 24.times do%li foo

    %div.col-md-3
      %i.glyphicon.glyphicon-plus
      = link_to 'Adicionar compromisso', [:new, :appointment]

      %div.datepicker
      %input(type='hidden' name='appointment[begin_at]')
